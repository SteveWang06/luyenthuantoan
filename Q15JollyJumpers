#include <iostream>
#include <vector>
#include <cmath>
#include <sstream>
#include <set>

using namespace std;

int main() {

    string line;
    while (getline(cin, line))
    {
        istringstream iss(line);

        int n;
        iss >> n;

        vector<int> seq(n);

        for (size_t i = 0; i < n; i++)
        {
            iss >> seq[i];
        }

        set<int> diffs;

        for (size_t i = 1; i < n; i++)
        {
            int diff = abs(seq[i] - seq[i - 1]);
            if (diff >= 1 && diff <= n - 1)
            {
                diffs.insert(diff);
            }
            

        }

        if ((int)diffs.size() == n - 1) cout << "Jolly" << endl;
        else cout << "Not jolly" << endl;
        
        
    }
    return 0;
}