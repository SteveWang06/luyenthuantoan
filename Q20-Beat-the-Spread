#include <iostream>

using namespace std;


int main() {

    int testCase, sum, diff;

    cin >> testCase;
    while (testCase--)
    {

        cin >> sum >> diff;

        if ((sum - diff) % 2 != 0 || (sum + diff) % 2 != 0 || sum < diff)
        {
            cout << "impossible" << endl;
        }
        else
        {
            int score1 = (sum + diff) / 2;
            int score2 = (sum - diff) / 2;

            cout << max(score1, score2) << " " << min(score1, score2) << endl;
        }

    }
    

    return 0;
}